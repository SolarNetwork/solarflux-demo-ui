{
  "name": "solarflux-demo-ui",
  "version": "1.0.9-dev.0",
  "description": "SolarFlux Demo UI",
  "keywords": [
    "solarnetwork",
    "solarflux"
  ],
  "homepage": "https://github.com/SolarNetwork/solarflux-demo-ui",
  "license": "Apache-2.0",
  "author": {
    "name": "SolarNetwork Foundation",
    "email": "code@solarnetwork.net"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SolarNetwork/solarflux-demo-ui.git"
  },
  "type": "module",
  "engines": {
    "node": ">=18.18"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "devbuild": "tsc && vite build --mode development",
    "preview": "vite preview",
    "release": "generate-release --current-version \"$(bin/remove-preid.cjs)\""
  },
  "config": {
    "generateRelease": {
      "no_confirm": false,
      "remote": "origin",
      "pre_commit_commands": [
        "npm run build"
      ],
      "post_complete_commands": [
        "npmversion --increment prepatch --preid dev --nogit-tag"
      ],
      "files_to_commit": []
    }
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.6",
    "cbor-sync": "^1.0.4",
    "d3-array": "^3.2.4",
    "d3-selection": "^3.0.0",
    "jquery": "^3.7.1",
    "paho-mqtt": "^1.1.0",
    "solarnetwork-api-core": "^3.3.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/bootstrap": "^5.2.10",
    "@types/crypto-js": "^4.2.2",
    "@types/d3-array": "^3.2.1",
    "@types/d3-selection": "^3.0.11",
    "@types/jquery": "^3.5.32",
    "@types/paho-mqtt": "^1.0.10",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^9.27.0",
    "generate-release": "^1.1.1",
    "npmversion": "^2.0.5",
    "sass": "^1.89.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "overrides": {
    "json5": "2.2.3",
    "lodash": "4.17.21",
    "minimist": "1.2.8",
    "postcss": "8.4.38",
    "semver": "7.6.0"
  }
}
